  <!-- Own CSS -->
	<link href="stylesheets/styleLogin.css" rel="stylesheet">
  
  <img src="./img/wiseMonkeyLogo.png" alt="logo">

  <div class="container">
    <div class="row loginContent">
      <div class="col-md-6 offset-md-3">
        <h1>Gorilla's In The Mist</h1>
        <p id="subTitle">Anonymous Web Chat Application</p>
        <span id="accessDenied" hidden>You Have An Existing Account</span>
        <form class="signup">
          <div class="form-group">
            <label for="exampleInputUsername1">Username</label>
            <input type="text" class="form-control" id="username-input" placeholder="Enter Username..." maxlength="8" data-toggle="tooltip" data-placement="top"
          title="Maximum Character of 8">
          </div>
          <div class="form-group">
            <label for="exampleInputPassword1">Password</label>
            <input type="password" class="form-control" id="password-input" placeholder="Enter Password...">
          </div>
          <div class="loginBtn">
            <button type="submit" class="btn btn-light">SIGN UP</button>
            <p class="loginSignUpHere">Or Login <a href="/login">Here</a></p>
          </div>
        </form>
      </div>
    </div>
  </div>